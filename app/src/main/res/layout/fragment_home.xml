<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.androidfinal.HomeFragment">

    <EditText
        android:id="@+id/edittexthomesearch"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="@string/Edit_Text"
        android:layout_alignParentTop="true"
        android:padding="5dp"
        />
    <EditText
        android:id="@+id/edittexthomesearching"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="@string/Edit_Text1"
        android:padding="5dp"
        android:layout_below="@+id/edittexthomesearch"
        />
    <Button
        android:id="@+id/buttonhomesearch"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/edittexthomesearching"
        android:text="@string/searchbutton"
        android:padding="5dp"
        />
    <ListView
        android:id="@+id/listviewhomelist"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/buttonhomesearch"
        android:padding="5dp"
        />

</RelativeLayout>